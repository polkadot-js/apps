      "@polkadot/react-components": [ "packages/react-components/src" ],
      "@polkadot/react-components/*": [ "packages/react-components/src/*" ],
      "@polkadot/react-hooks": [ "packages/react-hooks/src" ],
      "@polkadot/react-hooks/*": [ "packages/react-hooks/src/*" ],
      "@polkadot/react-params": [ "packages/react-params/src" ],
      "@polkadot/react-params/*": [ "packages/react-params/src/*" ],
      "@polkadot/react-query": [ "packages/react-query/src" ],
      "@polkadot/react-query/*": [ "packages/react-query/src/*" ],
      "@polkadot/react-signer": [ "packages/react-signer/src" ],
      "@polkadot/react-signer/*": [ "packages/react-signer/src/*" ],
      "@polkadot/test-support/": [ "packages/test-support/src/" ],
      "@polkadot/test-support/*": [ "packages/test-support/src/*" ]
    },
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@polkadot/ts",
      "./node_modules/@types"
    ]
  }
}
